ScreenManager:
    Initializer:
        id: init
    Screen:
        id: manager
        name: "manager"

<Initializer>:
    name: "initializer"
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    Image:
        allow_stretch: True
        keep_ratio: True
        source: "assets/N1d.PNG"
