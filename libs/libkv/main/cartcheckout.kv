<CartCheckOut>
    name: "checkout"
    RelativeLayout:
        opacity: 1 if not rv.data else 0
        MDIcon:
            icon:"cart-variant"
            color: app.theme_cls.disabled_hint_text_color
            font_size: dp(70)
            pos_hint: {"center_x":.5, "center_y":.5}
            halign: "center"
        MDLabel:
            text: "You have not added any product yet"
            color: app.theme_cls.disabled_hint_text_color
            halign: "center"
            pos_hint: {"center_x":.5, "center_y":.4}
    BoxLayout:
        orientation: "vertical"
        DoubleIconToolBar:
            icon_left: "arrow_left"
            icon_right: "magnify"
            title: "CheckOut"
            icon_left_callback: root.go_home